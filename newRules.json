{
	"rules":{
		".read": false,
		".write": "(auth!=null)",
		//Headlines
		"headlineOne":{
			".read":true,
			".validate": "(auth.uid == \ adminID)"
		},

		"headlineTwo":{
			".read":true,
			".validate": "(auth.uid == \ adminID)"
		},
		//log of all prizes given out 
		"logPile":{
			".read":true,
			".validate": "(auth.uid == \ adminID)&&(newData.hasChildren(['name','expTime']))",
			"name":{
				".validate":"(newData.isString())"
			},
			"expTime":{
				".validate":"(newData.isNumber())"
			}
		},

		//collection of users
		"hayStacks":{
			//for each user
			"$user" :{
				".read": "($user==auth.uid)",
				//userName
				
				//lastCheckedTime
				"creed" : {
					".validate": "((newData.exists())&&(auth.uid===$user)&&(newData.isNumber())&&(newData.val()==now)&&((!data.exists() || newData.val() > data.val()+100000 \ change value here",
					"$other": { ".validate": false }
				},

				
			}
		}
	}
}